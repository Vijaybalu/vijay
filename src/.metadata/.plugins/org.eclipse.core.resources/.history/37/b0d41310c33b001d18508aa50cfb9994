package Nested_classes;

public class LocalVariablesDemo {

	public static void main(String[] args) {
		String details="Details";
		int x=303;
		String name="vijay";
        char grade='A';
        short num=896;
        float points=7.3f;
        long number=8179719094l;
        byte no=7;
        
        System.out.println("my "+details+" are");
        System.out.println("--------------------");
        System.out.println("my roll no :"+x);
        System.out.println("my name is :"+name);
        System.out.println("my 10th grade is :"+grade);
        System.out.println("my inter marks are :"+num);
        System.out.println("my B.Tech percentage is :"+points+"%");
        System.out.println("my mobile number is :"+number);
        System.out.println("my lucky number is :"+no);
    }
  }
