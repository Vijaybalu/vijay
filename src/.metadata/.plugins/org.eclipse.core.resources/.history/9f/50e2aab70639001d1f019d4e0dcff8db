package Methodpoverriding;
class Weickle {
	void bike(String name,String color,int cost) {
		System.out.println("bike name is "+name);
	  	System.out.println("bike color is "+color);
	  	System.out.println("bike cost is "+cost); 
	}
}
class Bikes extends Weickle{
	void bike(String name,String color,int cost) {
	  	System.out.println("bike name is "+name);
	  	System.out.println("bike color is "+color);
	  	System.out.println("bike cost is "+cost); 
	}
	}


public class Methodpoverridingdemo1 {

	public static void main(String[] args) {
		Bikes b = new Bikes();
		b.bike("pulsar","black",100000);

	}

}
