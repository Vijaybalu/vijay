package THMES;
class Mist{
	void course(){
	System.out.println("mist college had more courses");
	b.branch();
	}
	class Btech{
	void branch() {
	System.out.println("mist college had more branches in b.tech");	
	}	
	}
	Btech b = new Btech();
}
public class Method_nested {

	public static void main(String[] args) {
		Mist m = new Mist();
		m.course();

	}

}
